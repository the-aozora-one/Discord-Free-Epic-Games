{
    "compilerOptions": {
        "module": "CommonJS",
        "lib": [
            "ES2020",
            "DOM",
        ],
        "target": "ES2023",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "outDir": "./dist",
        "esModuleInterop": true,
        "skipLibCheck": true,
    },
    "exclude": [
        "node_modules",
        "./node_modules",
        "./node_modules/*",
    ]
}